{
    "name": "aop/lalr",
    "description": "Lexing and parsing in pure PHP using LALR(1) parser",
    "keywords": ["lexing", "parsing", "ast", "parser"],
    "homepage": "https://github.com/php-aop/lalr",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub LÃ©dl",
            "email": "jakubledl@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "symfony/console": "^3.0|^4.0",
        "phpunit/phpunit": "^7.0"
    },
    "suggest": {
        "symfony/console": "for the command-line tool"
    },
    
    "bin": ["bin/lalr.php", "bin/lalr"],
    "autoload": {
        "files": [
            "src/Functions/has_diff.php",
            "src/Functions/substring.php",
            "src/Functions/union.php",
            "src/Functions/utf8_strlen.php",
            "src/Functions/hash_state_transitions.php"
        ],
        "psr-4": {
            "Aop\\LALR\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aop\\LALR\\Tests\\": "tests/"
        }
    }
}
